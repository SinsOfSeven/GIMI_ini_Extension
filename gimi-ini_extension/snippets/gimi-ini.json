{
	"Hollow GIMI": {
		"scope": "gimi-ini",
		"prefix": "hollow gimi",
		"body": [
			"",
			"; make by $1",
			"; for character $2",
			"",
			"; Constants ---------------------------",
			"",
			"; Overrides ---------------------------",
			"",
			"[TextureOverride$2]",
			"",
			"[ShaderOverride$2]",
			"",
			"; CommandList -------------------------",
			"",
			"; Resources ---------------------------",
			"",
			"[Resource$2.0]",
			"",
			"[ResourcesWhyYouDoThat]",
			"type = Buffer",
			"data = \"Seriously? Why you have typing the full .ini by hand?\"",
			"",
			"; .ini generated by GIMI (Genshin-Impact-Model-Importer) vscode extension",
			"; If you have any issues or find any bugs, good luck"
		],
		"description":"A Constants snippet"
	},
	"Dividing line": {
		"scope": "gimi-ini",
		"prefix": "dividing",
		"body": [
			"; $1 --------"
		],
		"description": "A dividing line can use on folding huge part"
	},
	"Default if condition": {
		"scope": "gimi-ini",
		"prefix": "if",
		"body": [
			"if ${TM_SELECTED_TEXT:$1}",
			"\t$0",
			"endif"
		],
		"description":"If Statement, support condition pre-selection"
	},
	"Default elif condition": {
		"scope": "gimi-ini",
		"prefix": "ifelse",
		"body": [
			"if ${TM_SELECTED_TEXT:$1}",
			"\t$0",
			"else",
			"\t",
			"endif"
		],
		"description":"If-Else Statement, support condition pre-selection"
	},
	"Test Opject": {
		"scope": "gimi-ini",
		"prefix": "just test",
		"body": [
			"${TM_CURRENT_LINE:$1}",
			"endtest"
		],
		"description":"Just a test object"
	}
}